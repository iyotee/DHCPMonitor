{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "DHCP Monitor Pro",
  "version": "0.0.0",
  "identifier": "com.dhcp-monitor.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "DHCP Monitor - Option 50 Tracker",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app", "dmg", "msi", "nsis", "appimage"],
    "icon": [
      "icons/icon.png",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "Copyright (c) 2024 DHCP Monitor",
    "category": "DeveloperTool",
    "shortDescription": "DHCP Monitor with Option 50 detection",
    "longDescription": "Modern DHCP monitoring application with specific Option 50 detection",
    "macOS": {
      "minimumSystemVersion": "10.13.0"
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  }
}