{
  "$schema": "https://schema.tauri.app/config/2",
  "version": "1.1.36",
  "identifier": "com.iyotee.dhcp-monitor",
  "productName": "DHCP Monitor Pro",
  "description": "DHCP Monitor - Option 50 Tracker",
  "author": "iyotee",
  "license": "MIT",
  "repository": "https://github.com/iyotee/DHCPMonitor",
  "defaultRun": "dev",
  "build": {
    "frontendDist": "../dist",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "DHCP Monitor Pro",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "resizable": true,
        "decorations": true,
        "transparent": false,
        "alwaysOnTop": false,
        "visible": true,
        "closable": true,
        "minimizable": true,
        "maximizable": true,
        "fullscreen": false,
        "skipTaskbar": false,
        "focus": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "identifier": "com.iyotee.dhcp-monitor",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}